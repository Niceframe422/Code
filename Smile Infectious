-- Load Infinite Yield & GUI
loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()

loadstring(game:HttpGet("https://pastebin.com/raw/TqgbdDjF"))()

-- Wait for GUI to load
task.wait(2)

-- Clean up GUI elements in CoreGui
local core = game:GetService("CoreGui")
local gui = core:FindFirstChild("ScreenGui")

-- Loop cleanup of GUI buttons except "close"
local function CleanGUI()
    if gui then
        for _, v in pairs(gui:GetChildren()) do
            if v.Name ~= "close" then
                pcall(function()
                    v:Destroy()
                end)
            end
        end
    end
end

-- Run GUI cleanup in loop
if gui then
    task.spawn(function()
        while task.wait(1) do
            CleanGUI()
        end
    end)
else
    warn("ScreenGui not found in CoreGui")
end

-- Dynamic remover for workspace objects
local keywords = {
    exact = {
        "AntiHack", "KillBricks", "Spawns", "Lava", "Weight", "Water", "KillZone","Flood"
    },
    contains = {
        "kill", "lava", "antihack"
    }
}

-- Check if a name matches any of the keywords
local function ShouldRemove(name)
    name = name:lower()
    for _, key in ipairs(keywords.exact) do
        if name == key:lower() then
            return true
        end
    end
    for _, part in ipairs(keywords.contains) do
        if name:find(part) then
            return true
        end
    end
    return false
end

-- Recursive search and remove matching objects
local function RemoveDangerousStuff()
    for _, obj in ipairs(game:GetService("Workspace"):GetDescendants()) do
        if ShouldRemove(obj.Name) or obj:IsA("BasePart") and obj.Color == Color3.fromRGB(213, 115, 61) then
            pcall(function()
                obj:Destroy()
            end)
        end
    end
end

spawn(function()
while wait(0.4) do
RemoveDangerousStuff()
end
end)

-- Load additional external scripts
task.wait(0.2)
loadstring(game:HttpGet("https://raw.githubusercontent.com/Niceframe422/Code/refs/heads/main/Fling%20Objects"))()
task.wait(0.2)
loadstring(game:HttpGet("https://pastefy.app/Vf5POrA6/raw"))()

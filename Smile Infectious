loadstring(game:HttpGet("https://pastebin.com/raw/TqgbdDjF"))()


local buttonsToKeep = {
    "Close", -- Replace with the name of your button
    "Button2", -- Add more button names as needed
}

-- Convert the list to a set for quick lookups
local buttonSet = {}
for _, buttonName in ipairs(buttonsToKeep) do
    buttonSet[buttonName] = true
end

-- Function to clean up ScreenGui objects in CoreGui
local function cleanCoreGui()
    local CoreGui = game:GetService("CoreGui")

    -- Iterate through all children of CoreGui
    for _, child in ipairs(CoreGui:GetChildren()) do
        if child:IsA("ScreenGui") then
            -- Iterate through all children of the ScreenGui
            for _, guiChild in ipairs(child:GetChildren()) do
                -- Remove the child if it's not a button in the set
                if not buttonSet[guiChild.Name] or not guiChild:IsA("GuiButton") then
                    guiChild:Destroy()
                end
            end
        end
    end
end

-- Run the cleanup function
cleanCoreGui()
